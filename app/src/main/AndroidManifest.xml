<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.escom.practica1activitiesfragments">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Detail Activities -->
        <activity
            android:name=".TextFieldsDetailActivity"
            android:exported="false" />

        <activity
            android:name=".ButtonsDetailActivity"
            android:exported="false" />

        <activity
            android:name=".SelectionDetailActivity"
            android:exported="false" />

        <activity
            android:name=".ListsDetailActivity"
            android:exported="false" />

        <activity
            android:name=".InfoElementsDetailActivity"
            android:exported="false" />

    </application>

</manifest>